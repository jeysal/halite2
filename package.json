{
  "private": true,
  "name": "halite2-bot",
  "version": "0.1.0",
  "author": "Tim Seckinger <seckinger.tim@gmail.com>",
  "license": "MIT",
  "main": "MyBot.js",
  "scripts": {
    "postinstall": "mkdirp dist replays tmp",
    "clean": "rm -rf *.log dist/* replays/* tmp",
    "all": "run-p typecheck lint test-once build-once",
    "typecheck": "tsc",
    "lint": "tslint --project tsconfig.json",
    "fix": "tslint --fix --project tsconfig.json",
    "test-once": "jest --coverage",
    "test": "jest --watch --coverage",
    "build-once": "webpack",
    "build": "webpack --watch",
    "start": "./halite -d '240 160' -i replays 'node dist/MyBot.js' 'node dist/MyBot.js' 'node dist/MyBot.js' 'node dist/MyBot.js'",
    "view": "electron node_modules/chlorine -o",
    "manager": "node_modules/halite/tools/manager/manager.py",
    "versions": "run-s add-versions run-versions",
    "clear-versions": "rm db.sqlite3",
    "add-versions": "./addVersions.groovy",
    "run-versions": "node_modules/halite/tools/manager/manager.py -fn"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "density-clustering": "^1.3.0",
    "halite": "HaliteChallenge/Halite-II#6c58f04",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.34",
    "@babel/core": "7.0.0-beta.34",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.34",
    "@babel/preset-env": "7.0.0-beta.34",
    "@babel/preset-typescript": "7.0.0-beta.34",
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.53",
    "@types/ramda": "^0.25.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "chlorine": "fohristiwhirl/chlorine#7a91ec6",
    "jest": "^21.2.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.8.2",
    "progress-bar-webpack-plugin": "^1.10.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
