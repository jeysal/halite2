{
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-console": true,
    "strict-boolean-expressions": false,
    "no-increment-decrement": false,
    "import-name": false,
    "ordered-imports": [true, { "grouped-imports": true }]
  }
}
